{
  "name": "resharper9-keybindings",
  "displayName": "Resharper 9 Keybindings",
  "version": "1.1.0",
  "description": "Port of Resharper 9 Keybindings",
  "bugs": {
    "url": "https://github.com/waderyan/vscode-resharper-keybindings/issues",
    "email": "wade.anderson@microsoft.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/waderyan/vscode-resharper-keybindings"
  },
  "license": "license.txt",
  "publisher": "waderyan",
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "contributes": {
    "keybindings": [
      {
        "command": "editor.action.copyLinesDownAction",
        "key": "ctrl+d",
        "mac": "cmd+d",
        "when": "editorTextFocus"
      },
      {
        "command": "editor.action.deleteLines",
        "key": "ctrl+l",
        "mac": "cmd+l",
        "when": "editorTextFocus"
      },
      {
        "command": "workbench.action.files.saveAll",
        "key": "ctrl+shift+s",
        "mac": "cmd+shift+s"
      },
      {
        "command": "workbench.action.files.revealActiveFileInWindows",
        "key": "ctrl+alt+l",
        "mac": "cmd+alt+l"
      },
      {
        "command": "workbench.action.quickOpen",
        "key": "ctrl+n",
        "mac": "cmd+n"
      },
      {
        "command": "editor.action.goToTypeDeclaration",
        "key": "ctrl+m",
        "mac": "cmd+m"
      },
      {
        "command": "workbench.action.navigateBack",
        "key": "ctrl+-",
        "mac": "cmd+-"
      },
      {
        "command": "workbench.action.gotoSymbol",
        "key": "ctrl+shift+alt+n",
        "mac": "ctrl+shift+alt+n"
      },
      {
        "command": "editor.action.smartSelect.grow",
        "key": "ctrl+w",
        "mac": "cmd+w",
        "when": "editorTextFocus"
      },
      {
        "command": "editor.action.smartSelect.shrink",
        "key": "ctrl+shift+w",
        "mac": "cmd+shift+w",
        "when": "editorTextFocus"
      },
      {
        "command": "editor.action.commentLine",
        "key": "ctrl+7",
        "mac": "cmd+7",
        "when": "editorTextFocus"
      }
    ]
  },
  "devDependencies": {
    "vscode": "0.11.0"
  },
  "icon": "keyboard.png",
  "galleryBanner": {
    "color": "#0000FF",
    "theme": "dark"
  }
}
